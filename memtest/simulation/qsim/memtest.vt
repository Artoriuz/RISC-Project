module memtest_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clear;
reg clk;
reg [1:0] operation;
// wires                                               
wire [7:0] out0;

wire sampler;                             

// assign statements (if any)                          
memtest i1 (
// port map - connection between master ports and signals/registers   
	.clear(clear),
	.clk(clk),
	.operation(operation),
	.out0(out0)
);

// clear
initial
begin
	clear = 1'b0;
end 

// clk
always
begin
	clk = 1'b0;
	clk = #10000 1'b1;
	#10000;
end 
// operation[ 1 ]
initial
begin
	operation[1] = 1'b0;
end 
// operation[ 0 ]
initial
begin
	operation[0] = 1'b0;
	operation[0] = #10000 1'b1;
	operation[0] = #20000 1'b0;
	operation[0] = #20000 1'b1;
	operation[0] = #40000 1'b0;
	operation[0] = #20000 1'b1;
	operation[0] = #40000 1'b0;
	operation[0] = #20000 1'b1;
	operation[0] = #40000 1'b0;
	operation[0] = #20000 1'b1;
end 

memtest_vlg_sample_tst tb_sample (
	.clear(clear),
	.clk(clk),
	.operation(operation),
	.sampler_tx(sampler)
);

memtest_vlg_check_tst tb_out(
	.out0(out0),
	.sampler_rx(sampler)
);
endmodule

