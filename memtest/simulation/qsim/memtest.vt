`timescale 1 ps/ 1 ps
module memtest_vlg_sample_tst(
	clear,
	clk,
	operation,
	sampler_tx
);
input  clear;
input  clk;
input [1:0] operation;
output sampler_tx;

reg sample;
time current_time;
always @(clear or clk or operation)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

